erRouter>\n          <div id=\"header-container\">\n            <Header />\n          </div>\n          <Switch>\n            <Route exact path='/' component={Home} />\n            <Route path='/random-quote-machine'component={RandomQuoteMachine} />\n            <Route path='/markdown-previewer' component={MarkdownPreviewer} />\n            <Route path='/drum-machine' component={DrumMachine} />\n            <Route path='/javascript-calculator' component={JavascriptCalculator} />\n            <Route path='/clock' component={ClockApp} />\n          </Switch>\n          <Footer />\n      </BrowserRouter>\n    );\n  };\n};\n\nexport default App;\n","/Users/nachocastillo/Desktop/Nacho/Estudios/freeCodeCamp/React FCC/fcc-react/src/components/Header.jsx",["211"],"import React from 'react';\nimport Logo from './Logo';\nimport Nav from './Nav';\nimport './header-style.css';\n\nclass Header extends React.Component {\n    constructor(props) {\n        super(props);\n    };\n\n    render() {\n        return(\n            <header>\n                <Logo />\n                <Nav />\n            </header>\n        );\n    };\n};\n\nexport default Header;","/Users/nachocastillo/Desktop/Nacho/Estudios/freeCodeCamp/React FCC/fcc-react/src/components/Footer.jsx",["212","213","214","215","216","217","218"],"import React from 'react';\nimport './footer-style.css';\n\nclass Footer extends React.Component {\n  constructor(props) {\n    super(props);\n  };\n\n  render() {\n    return (\n      <footer>\n        <div id=\"footer-container\">\n          <ul>\n            <li>\n              Ignacio Castillo\n            </li>\n            <li className=\"separator\">\n              //\n            </li>\n            <li>\n              <a href=\"https://www.linkedin.com/in/nachocast92\" target=\"_blank\">\n                💼 Linkedin\n              </a>\n            </li>\n            <li className=\"separator\">\n              //\n            </li>\n            <li>\n              <a href=\"https://github.com/neatdisorder\" target=\"_blank\">\n                🚀 Github\n              </a>\n            </li>\n            <li className=\"separator\">\n              //\n            </li>\n            <li>\n              <a href=\"https://www.freecodecamp.org/nachocast\" target=\"_blank\">\n                🔥 freeCodeCamp\n              </a>\n            </li>\n          </ul>\n        </div>\n      </footer>\n    );\n  };\n};\n\nexport default Footer;","/Users/nachocastillo/Desktop/Nacho/Estudios/freeCodeCamp/React FCC/fcc-react/src/components/drum-machine/DrumMachine.jsx",["219"],"import React from 'react';\nimport Drum from './Drum'\nimport DrumMachineHeader from './DrumMachineHeader';\n\nclass DrumMachine extends React.Component {\n  \n  constructor(props) {\n    super(props);\n  };\n  \n  render() {\n    return (\n      <div>\n        <DrumMachineHeader />\n        <Drum />\n      </div>\n    );\n  };\n    \n};\n\nexport default DrumMachine;","/Users/nachocastillo/Desktop/Nacho/Estudios/freeCodeCamp/React FCC/fcc-react/src/components/random-quote-machine/RandomQuoteMachine.jsx",["220"],"import React from 'react';\nimport QuoteContainer from './QuoteContainer';\nimport RandomQuoteMachineHeader from './RandomQuoteMachineHeader';\nimport QuoteButtons from './QuoteButtons';\nimport './random-quote-machine-style.css';\n\nclass RandomQuoteMachine extends React.Component {\n  \n    constructor(props) {\n      super(props);\n      this.state = {\n        quote: Math.floor(Math.random() * 5)\n      };\n      this.changeQuote = this.changeQuote.bind(this);\n    };\n    \n    changeQuote() {\n\n        let numberQuote = Math.floor(Math.random() * 5);\n\n        if (numberQuote === this.state.quote) {\n            if (numberQuote === 4) {\n                this.setState({\n                    quote: numberQuote - 1\n                });\n            } else {\n                this.setState({\n                    quote: numberQuote + 1\n                });\n            